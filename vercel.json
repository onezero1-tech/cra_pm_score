{
"version": 2,
"builds": [
{ "src": "api/main.py", "use": "@vercel/python" },
{ "src": "api/static/index.html", "use": "@vercel/static" }
],
"routes": [
{ "src": "/", "dest": "/api/static/index.html" },
{ "src": "/static/(.)", "dest": "/api/static/$1" },
{ "src": "/process", "dest": "/api/main.py" },
{ "src": "/merge", "dest": "/api/main.py" },
{ "src": "/(.)", "dest": "/api/main.py" }
]
}